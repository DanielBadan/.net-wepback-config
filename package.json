{
  "name": "index",
  "version": "0.1.0",
  "private": true,
  "homepage": "../",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.7",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.26.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "eslint": "3.8.1",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.10.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "http-proxy-middleware": "0.17.2",
    "imports-loader": "^0.7.0",
    "json-loader": "0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "raw-loader": "^0.5.1",
    "react-dev-utils": "^0.4.2",
    "recursive-readdir": "2.1.0",
    "script-loader": "^0.7.0",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.13.2",
    "url-loader": "0.5.7",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0",
    "write-file-webpack-plugin": "^3.4.2"
  },
  "dependencies": {},
  "scripts": {
    "start": "node npmScripts/start.js",
    "build": "node npmScripts/build.js"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-3"
    ],
    "plugins": [
      "transform-object-assign",
      "lodash"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "d3": true,
      "THREE": true,
      "$": true
    },
    "parser": "babel-eslint",
    "rules": {
      "eqeqeq": "warn",
      "no-eval": "error",
      "no-loop-func": "error",
      "no-return-assign": "warn",
      "no-console": "off",
      "no-debugger": "off",
      "strict": [
        "warn",
        "function"
      ],
      "array-bracket-spacing": "warn",
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "dot-notation": "error",
      "no-multi-spaces": "error"
    }
  }
}
